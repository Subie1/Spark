	"description": "The perfect messaging platform for the boys",
	"main": "index.js",
	"scripts": {
		"tauri": "tauri",
		"dev": "node .",
		"package": "node package.js",
		"build": "cd ./src/client/desktop && npm run build",
		"install-backend": "cd ./src/api && npm i",
		"install-frontend": "cd ./src/client/desktop && npm i",
		"install-all": "npm i && npm run install-backend && npm run install-frontend",
		"update-backend": "cd ./src/api && npm update",
		"update-frontend": "cd ./src/client/desktop && npm update",
		"update-all": "npm update && npm run update-backend && npm run update-frontend"
	},
